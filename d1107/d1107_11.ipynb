    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"score.csv\")\n",
    "df.set_index('지원번호',inplace=True)\n",
    "\n",
    "# df\n",
    "\n",
    "df[df['이름'].str.startswith('강')]\n",
    "filt = df['이름'].str.startswith('강')\n",
    "df[filt]\n",
    "df[~filt]\n",
    "\n",
    "filt = df['이름'].str.contains('강')\n",
    "df[filt]\n",
    "df[~filt]\n",
    "\n",
    "\n",
    "\n",
    "\n"
